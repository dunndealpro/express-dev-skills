<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h1>Todos</h1>
    <ul>
        <% skills.forEach(function(s) { %>
            <li>
                <a href="/skills/<%= s.id%>">
                    <%= s.skill %>
                </a>
                <%= s.skill %>
                    -
                    <%= s.skill ? 'have' : 'do not have' %>
            </li>
            <% }); %>
    </ul>
</body>

</html>